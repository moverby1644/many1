<h1>Editing Bmark #<%= @bmark.id %></h1>

<form action="/bmarks/<%= @bmark.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @bmark.name  %>" type="text" />
  </div>
  <div>
    <label for="url">url</label><br />
    <input id="url" name="url" value="<%= @bmark.url  %>" type="text" />
  </div>
  <div>
    <label for="desc">desc</label><br />
    <input id="desc" name="desc" value="<%= @bmark.desc  %>" type="text" />
  </div>
  <div>
    <label for="user_id">user_id</label><br />
    <input id="user_id" name="user_id" value="<%= @bmark.user_id  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


